@model List<OnlineResultCheckPortal.JSCEResult>
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
   
</head>
<body>
    <div class="container">
        <h2>Results Masta</h2>
        <table width="100%" class="table table-bordered" cellpadding="3" cellspacing="3" widths="30;60" borderwidth="1.0" left="false" right="false" top="false" bottom="false" red="0" green="0" blue="0">
            <tr>
                <td><b>Grade</b></td>

                <td><b>Remarks</b></td>
                <td><b>SubjectName</b></td>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="font-weight:normal">@item.Grade</td>

                    <td>@item.Remarks</td>
                    <td>@item.SubjectName</td>
                </tr>
            }
        </table>
        </div>
</body>
</html>
